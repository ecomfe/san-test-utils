{
  "name": "san-test-utils",
  "version": "0.1.3",
  "description": "Utilities for testing San Components.",
  "main": "dist/san-test-utils.js",
  "files": [
    "dist/*.js"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest ./test/specs/",
    "release": "sh scripts/release.sh",
    "doc:dev": "san docit ./docs/zh-cn/",
    "doc:release": "san docit ./docs/zh-cn/ --output --mode production --config ./docs/san.config.js"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ecomfe/san-test-utils.git"
  },
  "author": "mayihui",
  "license": "MIT",
  "peerDependencies": {
    "cheerio": "^1.0.0-rc.3",
    "san": "^3.8.1",
    "san-ssr": "^1.10.0"
  },
  "dependencies": {
    "dom-event-types": "^1.0.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "jest": "^24.9.0",
    "jest-babel": "^1.0.1",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "san-cli": "^0.1.3",
    "sinon": "^8.0.1",
    "webpack": "^4.41.4"
  }
}
